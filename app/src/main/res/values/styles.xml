    <style name="TSUButton.WrapContent.Round">
        <item name="cornerRadius">8dp</item>
    </style>

    <style name="TSUButton.WrapContent.Profile">
        <item name="android:minHeight">40dp</item>
        <item name="cornerRadius">4dp</item>
    </style>

    <style name="TSUButton.Support" parent="TSUButton.WrapContent.Profile">
        <item name="backgroundTint">@color/profileButtonSecondary</item>
        <item name="iconTint">@color/secondary2</item>
        <item name="android:textColor">@color/profileButtonText</item>
    </style>

    <style name="TSUButton.Grey" parent="TSUButton.WrapContent.Profile">
        <item name="backgroundTint">@color/profileButtonSecondary</item>
        <item name="iconTint">@color/colorPrimary</item>
        <item name="android:textColor">@color/colorPrimary</item>
    </style>

    <style name="TSUButton.WrapContent.Rounded" parent="TSUButton.WrapContent">
        <item name="cornerRadius">@dimen/button_corners_rounded</item>
    </style>

    <style name="TSUButton.WrapContent.SupportMe" parent="TSUButton.WrapContent.Rounded">
