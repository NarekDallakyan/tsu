        <item name="android:minWidth">40dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:paddingEnd">@dimen/button_padding_default</item>
        <item name="android:paddingLeft">@dimen/button_padding_default</item>
        <item name="android:paddingRight">@dimen/button_padding_default</item>
        <item name="android:paddingStart">@dimen/button_padding_default</item>
        <item name="android:paddingTop">4dp</item>
