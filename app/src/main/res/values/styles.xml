        <item name="android:paddingLeft">@dimen/field_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/field_padding_horizontal</item>
        <item name="android:paddingTop">@dimen/field_padding_vertical</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">@color/field_multiline_hint</item>
        <item name="fontFamily">@font/lato_bold</item>
    </style>

    <style name="TSUTextAppearance">
        <item name="fontFamily">@font/lato</item>
        <item name="android:fontFamily">@font/lato</item>
    </style>

    <style name="TSUTextAppearance.Medium">
