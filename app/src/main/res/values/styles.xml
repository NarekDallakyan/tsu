        <item name="android:paddingTop">@dimen/field_padding_vertical</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">@color/field_multiline_hint</item>
        <item name="fontFamily">@font/lato_bold</item>
    </style>

    <style name="TSUEditText.Multiline" parent="Widget.AppCompat.EditText">
        <item name="android:backgroundTint">@android:color/transparent</item>
        <item name="android:fontFamily">@font/lato_bold</item>
        <item name="android:gravity">start|top</item>
        <item name="android:inputType">textMultiLine</item>
        <item name="android:layout_marginBottom">8dp</item>
        <item name="android:minLines">@dimen/create_post_field_min_lines</item>
