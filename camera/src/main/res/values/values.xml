    <declare-styleable name="CameraView">

        <attr format="integer|reference" name="cameraPictureSizeMinWidth"/>
        <attr format="integer|reference" name="cameraPictureSizeMaxWidth"/>
        <attr format="integer|reference" name="cameraPictureSizeMinHeight"/>
        <attr format="integer|reference" name="cameraPictureSizeMaxHeight"/>
        <attr format="integer|reference" name="cameraPictureSizeMinArea"/>
        <attr format="integer|reference" name="cameraPictureSizeMaxArea"/>
        <attr format="boolean" name="cameraPictureSizeSmallest"/>
        <attr format="boolean" name="cameraPictureSizeBiggest"/>
        <attr format="string|reference" name="cameraPictureSizeAspectRatio"/>

        <attr format="integer|reference" name="cameraVideoSizeMinWidth"/>
        <attr format="integer|reference" name="cameraVideoSizeMaxWidth"/>
        <attr format="integer|reference" name="cameraVideoSizeMinHeight"/>
        <attr format="integer|reference" name="cameraVideoSizeMaxHeight"/>
        <attr format="integer|reference" name="cameraVideoSizeMinArea"/>
        <attr format="integer|reference" name="cameraVideoSizeMaxArea"/>
